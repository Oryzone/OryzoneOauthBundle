{
    "name": "oryzone/oauth-bundle",
    "type": "symfony-bundle",
    "description": "Symfony2 Oauth bundle using PHPoAuthLib",
    "keywords": ["oauth", "authentication", "authorization", "security", "bundle"],
    "homepage": "https://github.com/Oryzone/OryzoneOauthBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Luciano Mammino",
            "email": "lmammino@oryzone.com",
            "homepage": "http://oryzone.com",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.0",
        "symfony/framework-bundle": ">=2.1,<3.0-dev",
        "oryzone/oauth-user-data": "~1.0@dev"
    },
    "require-dev" : {
        "phpunit/phpunit": "~3.7"
    },
    "autoload": {
        "psr-4": { "Oryzone\\Bundle\\OauthBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
